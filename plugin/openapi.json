{
  "openapi": "3.0.1",
  "info": {
    "title": "Calendar AI API",
    "description": "API for managing Google Calendar events",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://YOUR_DOMAIN"
    }
  ],
  "paths": {
    "/list_events": {
      "get": {
        "summary": "List upcoming 10 events",
        "operationId": "listEvents",
        "responses": {
          "200": {
            "description": "List of events"
          }
        }
      }
    },
    "/create_event": {
      "post": {
        "summary": "Create a new event",
        "operationId": "createEvent",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "summary": { "type": "string" },
                  "start": { "type": "string" },
                  "end": { "type": "string" }
                },
                "required": ["summary", "start", "end"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Event created"
          }
        }
      }
    }
  }
}

